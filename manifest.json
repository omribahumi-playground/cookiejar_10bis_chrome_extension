{
  "manifest_version": 2,

  "name": "Everything.me Cookiejar",
  "description": "This extension lets you share session cookies with your fellow geeks.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "https://evmecookie.appspot.com/",
    "http://10bis.co.il/",
    "http://www.10bis.co.il/"
  ]
}
